{
  "name": "bunny",
  "version": "1.0.0",
  "description": "",
  "main": "capture.js",
  "scripts": {
    "build": "docker build -t bunny .",
    "docker-run": "docker run --init --rm -e DISPLAY=:99 -e DISPLAY_WIDTH=1280 -e DISPLAY_HEIGHT=720 --name=bunny -p 8080:8080 -p 3000:3000 bunny",
    "start": "npm run build && npm run docker-run",
    "restart": "docker start -a bunny",
    "stop": "docker stop bunny -t 1",
    "connect": "docker container exec -it bunny /bin/bash"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.12.11"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "mp4frag": "^0.2.0",
    "simple-peer": "^9.6.2",
    "socket.io": "^2.3.0",
    "wrtc": "^0.4.3"
  }
}
